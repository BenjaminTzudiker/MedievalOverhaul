<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="MedievalOverhaul.PatchOperation_ToggleSettings">
		<settings>
			<li>boomalopeTar</li>
		</settings>
		<active Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef/comps/li[@Class="CompProperties_Milkable"]/milkDef[.="Chemfuel"]</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/ThingDef/comps/li[@Class="CompProperties_Milkable"]/milkDef[.="Chemfuel"]</xpath>
						<value>
							<milkDef>DankPyon_Tar</milkDef>
						</value>
					</match>
				</li>

			</operations>
		</active>
	</Operation>

</Patch>